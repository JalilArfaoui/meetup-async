{
  "name": "meetup-async",
  "version": "1.0.0",
  "description": "demo app for redux saga",
  "main": "./server/boot.js",
  "scripts": {
    "test": "test",
    "start": "node ./server/boot.js"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 1%",
              "safari >= 10"
            ],
            "node": "current"
          }
        }
      ],
      "react",
      "stage-3",
      "flow"
    ],
    "plugins": [
      "transform-runtime",
      "transform-class-properties"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "standard": {
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "ignore": [
      "/doc/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moe971/meetup-async.git"
  },
  "author": "Moe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moe971/meetup-async/issues"
  },
  "homepage": "https://github.com/moe971/meetup-async#readme",
  "dependencies": {
    "attr-accept": "^1.1.0",
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.9",
    "express-fileupload": "^0.4.0",
    "postcss-loader": "^2.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.8",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-observable": "^0.18.0",
    "redux-saga": "^0.16.0",
    "rxjs": "^5.5.6",
    "style-loader": "^0.20.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-3": "^6.24.1",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.4.1",
    "express": "^4.16.2",
    "flow-bin": "^0.65.0",
    "standard": "^10.0.3",
    "webpack": "^3.11.0",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-hot-middleware": "^2.21.0"
  }
}
